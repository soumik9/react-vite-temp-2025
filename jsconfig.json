{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@src/*": [
        "*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@libs/*": [
        "libs/*"
      ],
      "@hooks/*": [
        "libs/hooks/*"
      ],
      "@router/*": [
        "libs/router/*"
      ],
      "@styles/*": [
        "libs/styles/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@assets/*": [
        "assets/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}